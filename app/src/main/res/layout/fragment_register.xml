<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    tools:context=".Register">


    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:width="340dp"
        android:height="50dp"
        android:background="@drawable/bg_gray"
        android:ems="10"
        android:fontFamily="@font/dm_sans"
        android:inputType="textPersonName"
        android:lineHeight="25dp"
        android:minHeight="48dp"
        android:paddingLeft="29dp"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        android:text="Full Name"
        android:textColor="#2C2948"
        android:textFontWeight="400"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:width="340dp"
        android:height="50dp"
        android:background="@drawable/bg_gray"
        android:ems="10"
        android:fontFamily="@font/dm_sans"
        android:inputType="textPersonName"
        android:lineHeight="25dp"
        android:minHeight="48dp"
        android:paddingLeft="29dp"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        android:text="Full Name"
        android:textColor="#2C2948"
        android:textFontWeight="400"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="340dp"
        android:height="50dp"
        android:background="@drawable/bg_gray"
        android:ems="10"
        android:fontFamily="@font/dm_sans"
        android:inputType="textPersonName"
        android:lineHeight="25dp"
        android:minHeight="48dp"
        android:paddingLeft="29dp"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        android:text="Full Name"
        android:textColor="#2C2948"
        android:textFontWeight="400"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:width="340sp"
        android:height="60sp"
        android:background="@drawable/editbg"
        android:text="Sign up my Account"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:width="340sp"
        android:height="60sp"
        android:background="@drawable/bg_dark_blue"
        android:text="Sign up with Phone Number"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:buttonTint="#7165E3"
        android:fontFamily="@font/dm_sans_medium"
        android:lineHeight="18dp"
        android:minHeight="48dp"
        android:text="By creating your account you have to agree\n with our Teams and Conditions."
        android:textColor="#CC1C1939"
        android:textFontWeight="400"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:width="170dp"
        android:height="46dp"
        android:fontFamily="@font/dm_sans_bold"
        android:lineHeight="45.57dp"
        android:text="Welcome!"
        android:textAlignment="center"
        android:textColor="#1c1939"
        android:textFontWeight="700"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dm_sans"
        android:lineHeight="25dp"
        android:text="Please provide following\ndetails for your new account"

        android:textAlignment="center"
        android:textColor="#CC1C1939"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/dm_sans"
        android:lineHeight="25dp"
        android:text="Already have an account?"
        android:textAlignment="center"
        android:textColor="#CC1C1939"
        android:textFontWeight="400"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:fontFamily="@font/dm_sans"
        android:lineHeight="25dp"
        android:text=" - Sign In"
        android:textAlignment="center"
        android:textColor="#CC1C1939"
        android:textFontWeight="700"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />


</androidx.constraintlayout.widget.ConstraintLayout>