<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout16"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestReportForTeacher">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="300dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/animationView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#03A9F4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:lottie_autoPlay="true"
            app:lottie_fileName="celebration.json"
            app:lottie_loop="true" />

        <ImageView
            android:id="@+id/backbtn"
            android:layout_width="26dp"
            android:layout_height="18dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/back"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/top1"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:civ_border_color="#E91E63"
            app:civ_border_width="0.5dp"
            app:civ_shadow="true"
            app:civ_shadow_color="#757575"
            app:civ_shadow_radius="0.5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.120000005"
            app:srcCompat="@drawable/graduated" />

        <TextView
            android:id="@+id/tvtop2name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            android:text="Name"
            android:textColor="#FAF9F9"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/top2"
            app:layout_constraintStart_toStartOf="@+id/top2"
            app:layout_constraintTop_toBottomOf="@+id/top2" />

        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/top2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:civ_border_color="#E91E63"
            app:civ_border_width="0.5dp"
            app:civ_shadow="true"
            app:civ_shadow_color="#757575"
            app:civ_shadow_radius="1dp"
            app:layout_constraintEnd_toStartOf="@+id/top1"
            app:layout_constraintHorizontal_bias="0.35"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/top1"
            app:srcCompat="@drawable/graduated">
        </com.mikhaellopez.circularimageview.CircularImageView>

        <TextView
            android:id="@+id/tvtop1name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            android:text="Name"
            android:textColor="#FAF9F9"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/top1"
            app:layout_constraintStart_toStartOf="@+id/top1"
            app:layout_constraintTop_toBottomOf="@+id/top1" />

        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/top3"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:civ_border_color="#E91E63"
            app:civ_border_width="0.5dp"
            app:civ_shadow="true"
            app:civ_shadow_color="#757575"
            app:civ_shadow_radius="1dp"
            app:layout_constraintEnd_toEndOf="@+id/animationView"
            app:layout_constraintHorizontal_bias="0.83"
            app:layout_constraintStart_toEndOf="@+id/top1"
            app:layout_constraintTop_toBottomOf="@+id/top1"
            app:srcCompat="@drawable/graduated" />

        <TextView
            android:id="@+id/tvtop3name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            android:text="Name"
            android:textColor="#FAF9F9"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/top3"
            app:layout_constraintStart_toStartOf="@+id/top3"
            app:layout_constraintTop_toBottomOf="@+id/top3" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/imageView14"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/top3"
            app:layout_constraintHorizontal_bias="0.398"
            app:layout_constraintStart_toEndOf="@+id/top2"
            app:layout_constraintTop_toBottomOf="@+id/tvtop1name"
            app:lottie_autoPlay="true"
            app:lottie_fileName="prize.json"
            app:lottie_loop="true" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/btnscoreboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-25dp"
        android:width="340sp"
        android:height="60sp"
        android:background="@drawable/editbg"
        android:gravity="center"
        android:text="View Score Board"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:paddingStart="6dp"
        android:text="Test Summary"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnscoreboard" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27">

        <androidx.cardview.widget.CardView
            android:layout_width="162dp"
            android:layout_height="144dp"
            android:layout_margin="6dp"
            android:padding="6dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="3dp"
            app:cardMaxElevation="3dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="6dp">

                <me.bastanfar.semicirclearcprogressbar.SemiCircleArcProgressBar
                    android:id="@+id/prhighscore"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"

                    app:progressBarColor="#048C7F"
                    app:progressBarWidth="15"
                    app:progressPlaceHolderColor="#7E7E7E"
                    app:progressPlaceHolderWidth="15" />

                <TextView
                    android:id="@+id/tvhighscore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="40%"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="@+id/prhighscore"
                    app:layout_constraintEnd_toEndOf="@+id/prhighscore"
                    app:layout_constraintStart_toStartOf="@+id/prhighscore"
                    app:layout_constraintTop_toTopOf="@+id/prhighscore"
                    app:layout_constraintVertical_bias="0.64" />

                <TextView
                    android:id="@+id/tvhighscoremarks"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="High Score"
                    android:textColor="#616161"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/prhighscore" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="162dp"
            android:layout_height="144dp"
            android:layout_margin="6dp"
            android:padding="6dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="3dp"
            app:cardMaxElevation="3dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="6dp">

                <me.bastanfar.semicirclearcprogressbar.SemiCircleArcProgressBar
                    android:id="@+id/prattempt"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:percent="30"
                    app:progressBarColor="#048C7F"
                    app:progressBarWidth="15"
                    app:progressPlaceHolderColor="#7E7E7E"
                    app:progressPlaceHolderWidth="15" />

                <TextView
                    android:id="@+id/tvattemptstudent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#3A3A3A"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/prattempt"
                    app:layout_constraintEnd_toEndOf="@+id/prattempt"
                    app:layout_constraintStart_toStartOf="@+id/prattempt"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.69" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Students \nAttempet in test"
                    android:textColor="#616161"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/prattempt" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        app:layout_constraintVertical_bias="0.04000002">

        <androidx.cardview.widget.CardView
            android:layout_width="162dp"
            android:layout_height="144dp"
            android:layout_margin="6dp"
            android:padding="6dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="3dp"
            app:cardMaxElevation="3dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="6dp">

                <me.bastanfar.semicirclearcprogressbar.SemiCircleArcProgressBar
                    android:id="@+id/prclasstop10score"
                    android:layout_width="0dp"
                    android:layout_height="90dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:percent="30"
                    app:progressBarColor="#048C7F"
                    app:progressBarWidth="15"
                    app:progressPlaceHolderColor="#7E7E7E"
                    app:progressPlaceHolderWidth="15" />

                <TextView
                    android:id="@+id/tvtop10score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="200/250"
                    android:textColor="#090808"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/prclasstop10score"
                    app:layout_constraintEnd_toEndOf="@+id/prclasstop10score"
                    app:layout_constraintStart_toStartOf="@+id/prclasstop10score"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.69" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Class Top 10 Score"
                    android:textColor="#616161"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/prclasstop10score" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>